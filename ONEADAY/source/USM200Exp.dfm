  object qryOpen: TQuery
    DatabaseName = 'ONEADAY'
    SQL.Strings = (
      'SELECT PNO'
      '      ,CUST_CODE'
      '      ,ITEM_NAME'
      
        '      ,substring(CONVERT([varchar](20),cast(IMP_EXPDATE as datet' +
        'ime),(120)),1,10) IMP_EXPDATE'
      '      ,CODE_SNM'
      '      ,IMP_EXPQTY'
      '      ,IMP_QTY'
      '      ,EXP_QTY'
      '      ,RET_QTY'
      '      ,CHN_QTY'
      '      ,BACK_QTY'
      '      ,a.DANGA'
      '      ,a.TAX_GUBUN'
      '      ,CUST_ZIP'
      '      ,CUST_ADDRESS'
      '      ,CUST_TEL1'
      '      ,CUST_TEL2'
      '      ,CUST_PER'
      '      ,a.I_DATE'
      '      ,a.I_ID'
      '      ,a.U_DATE'
      '      ,a.U_ID'
      '      ,a.ITEM_CODE'
      '  FROM SM200 a'
      '  JOIN MT200 b'
      '    ON a.ITEM_CODE = b.ITEM_CODE'
      '  JOIN MT300 c'
      '    ON a.SET_STAT = c.CODE_CD    '
      '   AND c.CODE_TP = '#39'0001'#39
      '   AND c.CODE_CD <> '#39'00'#39
      'WHERE (c.CODE_CD = '#39'01'#39' OR c.CODE_CD = '#39'02'#39')')
    Left = 88
    Top = 176
  end
