  object qryOpen: TQuery
    DatabaseName = 'DAIHAN'
    SQL.Strings = (
      
        'SELECT substring(LOC_CODE,1,1)+'#39'-'#39'+substring(LOC_CODE,2,2)+'#39'-'#39'+s' +
        'ubstring(LOC_CODE,4,2)+'#39'-'#39'+substring(LOC_CODE,6,1)'
      '      ,a.ITEM_CODE'
      '      ,ITEM_NAME '
      '      ,a.I_DATE'
      '      ,a.I_ID'
      '      ,a.U_DATE'
      '      ,a.U_ID'
      '  FROM dbo.PD710 a'
      '  JOIN dbo.MT300 b'
      '    ON a.ITEM_CODE = b.ITEM_CODE'
      'ORDER BY a.ITEM_CODE')
    Left = 88
    Top = 176
  end
