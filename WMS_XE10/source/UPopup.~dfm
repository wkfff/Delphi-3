  object qryOpen: TQuery
    DatabaseName = 'DAIHAN'
    SQL.Strings = (
      'SELECT ITEM_CODE'
      '      ,ITEM_NAME'
      '      ,ITEM_GUBUN'
      '      ,UNIT'
      '      ,UNIT_CS'
      '      ,UNIT_PK'
      '      ,KAN_CODE'
      '      ,CATNO'
      '      ,I_DATE'
      '      ,I_ID'
      '      ,U_DATE'
      '      ,U_ID'
      '  FROM dbo.MT300')
    Left = 16
    Top = 216
  end
end
