  object qryOpen: TQuery
    DatabaseName = 'DAIHAN'
    SQL.Strings = (
      
        'SELECT D.LOC_CODE, MAX(B.ITEM_CODE) ITEM_CODE, MAX(C.ITEM_NAME) ' +
        'ITEM_NAME, MAX(B.ORDER_UNIT) ORDER_UNIT,'
      
        '  SUM(B.QTY) QTY, D.EXP_DATE, SUM(D.INOUT_QTY), MAX(B.CUST_CODE)' +
        ' CUST_CODE, E.CUST_NAME'
      'FROM ('
      '  SELECT ORDER_NO FROM PM100'
      '  WHERE ORDER_STAT IN ( '#39'20'#39', '#39'25'#39')'
      ' AND ORDER_TYPE = '#39'2000'#39' ) A,'
      '  PM100 B, MT300 C, PD120 D, MT200 E'
      'WHERE '
      '  D.LOC_CODE like :LOC_CODE'
      '  AND A.ORDER_NO = B.ORDER_NO'
      '  AND B.ITEM_CODE = C.ITEM_CODE'
      '  AND B.ORDER_TYPE = D.ORDER_TYPE'
      '  AND B.ORDER_NO = D.ORDER_NO'
      '  AND B.ITEM_CODE = D.ITEM_CODE'
      '  AND B.CUST_CODE = E.CUST_CODE '
      '  AND B.ORDER_UNIT = D.ORDER_UNIT'
      ' AND C.ITEM_GUBUN like :ITEM_GUBUN'
      '  AND D.EXP_DATE between :FR_DATE and :TO_DATE'
      
        'GROUP BY D.LOC_CODE, B.ITEM_CODE, B.ORDER_UNIT, D.EXP_DATE, E.CU' +
        'ST_NAME'
      'ORDER BY D.LOC_CODE, B.ITEM_CODE, B.ORDER_UNIT')
    Left = 48
    Top = 208
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'LOC_CODE'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'ITEM_GUBUN'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'FR_DATE'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'TO_DATE'
        ParamType = ptUnknown
      end>
  end
